{
    "name": "nazonohito51/cakephp2-ide-helper",
    "description": "Improve auto-completion for cakephp2",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "nazonohito51",
            "email": "nazonohito51@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CakePhp2IdeHelper\\": "src/"
        }
    },
    "autoload-dev": {
